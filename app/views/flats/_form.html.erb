<%= simple_form_for [@flat] do |f|%>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :city %>
    <%= f.input :max_guest %>
    <%= f.input :price_per_night %>
    <%= f.input :description %>
    <%= f.file_field :photos, multiple: true %>
    <%= f.button :submit, class: 'btn btn-primary' %>
    <% end %>
